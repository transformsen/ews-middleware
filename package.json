{
  "name": "ews",
  "version": "1.0.0",
  "description": "Exchange Web Services (EWS) Middleware",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -l",
    "prettier:fix": "npm run prettier:cli --write",
    "eslint": "lb-eslint .",
    "eslint:fix": "npm run eslint --fix",
    "pretest": "npm run clean && npm run build",
    "migrate": "node ./dist/migrate",
    "start": "npm run build && node -r source-map-support/register .",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.0.1",
    "@loopback/context": "^3.1.0",
    "@loopback/core": "^2.1.0",
    "@loopback/eslint-config": "^6.0.1",
    "@loopback/openapi-v3": "^3.1.0",
    "@loopback/repository": "^2.0.1",
    "@loopback/rest": "^3.0.1",
    "@loopback/rest-explorer": "^2.0.1",
    "@loopback/service-proxy": "^2.0.1",
    "loopback-connector-rest": "^3.6.0",
    "strong-soap": "^1.22.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.6",
    "@loopback/testlab": "^2.0.1",
    "@types/jest": "^25.1.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "typescript": "~3.8.3"
  }
}
